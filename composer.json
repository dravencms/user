{
  "name": "dravencms/user",
  "type": "dravencms-package",
  "description": "User module for dravencms",
  "keywords": [
    "dravencms",
    "user"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "nette/security": "^3.1.4",
    "dravencms/application": ">=0.0.2",
    "dravencms/database": ">=0.0.3",
    "dravencms/base-form": ">=0.0.1",
    "dravencms/base-grid": ">=0.0.1",
    "dravencms/templated-email": ">=v1.0",
    "dravencms/captcha": ">=0.0.2",
    "dravencms/locale": ">=v1.1.1"
  },
  "replace": {
    "dravencms/user-light": "*"
  },
  "conflicts": {
    "dravencms/user-light": "*"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": "dravencms.config.neon"
    }
  }
}
